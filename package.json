{
  "name": "tlgrupos",
  "version": "1.2.1",
  "description": "Sistema de gerenciamento de membros do Telegram com controle de vencimento",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "cron:check-expired": "tsx scripts/check-expired-members.ts",
    "cron:send-notifications": "tsx scripts/send-expiry-notifications.ts",
    "cron:process-payments": "curl -X POST http://localhost:3001/api/processar-pagamentos",
    "setup:bot": "tsx scripts/setup-bot.ts",
    "sync:members": "node -r dotenv/config scripts/sync-group-members-wrapper.js",
    "get-group-id": "node scripts/get-group-id-wrapper.js",
    "get-updates": "node scripts/get-updates-wrapper.js",
    "start:bot": "node scripts/start-bot-wrapper.js",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "pg": "^8.16.3",
    "pix-utils": "^2.8.2",
    "postcss": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "resend": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "telegraf": "^4.16.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "bmad-method": "^4.44.3",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0"
  },
  "keywords": [
    "telegram",
    "supabase",
    "automation",
    "membership"
  ],
  "author": "",
  "license": "ISC"
}
